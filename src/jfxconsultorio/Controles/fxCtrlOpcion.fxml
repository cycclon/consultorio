            <Tooltip text="" fx:id="tlNotificaciones" />
          </tooltip>
        </Label>
      </children>
    </StackPane>
    <Label fx:id="lblNombre" alignment="CENTER" styleClass="labelNombre" text="Nombre de opcion" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
