                  <VBox fx:id="vbMovimientos" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
               </content>
            </ScrollPane>
            <GridPane hgap="10.0">
               <children>
                  <Label text="Saldo actual:" />
                  <Label fx:id="lblSaldoFinal" styleClass="LabelResaltado" text="Label" GridPane.columnIndex="1">
